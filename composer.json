{
    "name": "avunu/wp-cloud-files",
    "description": "Use S3 object storage for WordPress uploads",
    "type": "library",
    "require": {
        "php": "^8.1",
        "league/flysystem-aws-s3-v3": "~3.29.0",
        "aws/aws-sdk-php": "^3.342",
        "phpoffice/phpword": "^1.3.0",
        "phpoffice/phpspreadsheet": "^3.9.1",
        "phpoffice/phppresentation": "dev-master",
        "dompdf/dompdf": "^3.1.0",
        "mpdf/mpdf": "^8.2.5"
    },
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Avunu LLC",
            "email": "mail@avu.nu"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Avunu\\WPCloudFiles\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "classmap-authoritative": true
    }
}
